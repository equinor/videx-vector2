{
  "name": "@equinor/videx-vector2",
  "version": "1.0.45",
  "description": "Vector2 class library written in javascript.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "copyfiles images/* docs && copyfiles .nojekyll docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-vector2.git"
  },
  "keywords": [
    "videx",
    "vector",
    "2d"
  ],
  "author": "Tom Kristian Tjemsland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-vector2/issues"
  },
  "homepage": "https://github.com/equinor/videx-vector2#readme",
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "copyfiles": "^2.2.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^3.29.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^29.5.0",
    "rimraf": "^3.0.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@equinor/videx-linear-algebra": "^1.0.10"
    },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
