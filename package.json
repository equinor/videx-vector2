{
  "name": "@equinor/videx-vector2",
  "version": "1.0.48",
  "description": "Vector2 class library written in javascript.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "node postdocs.copyfiles.mjs",
    "lint": "eslint --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-vector2.git"
  },
  "keywords": [
    "videx",
    "vector",
    "2d"
  ],
  "author": "Tom Kristian Tjemsland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-vector2/issues"
  },
  "homepage": "https://github.com/equinor/videx-vector2#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.5",
    "ts-jest": "^29.1.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@equinor/videx-linear-algebra": "^1.0.10"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
